hyper - 0x21 : yabai -m space --focus prev # [
hyper - 0x1E : yabai -m space --focus next # ]

hyper - t : yabai -m window --toggle float

hyper - h : yabai -m window --focus west
hyper - j : yabai -m window --focus south
hyper - k : yabai -m window --focus north
hyper - l : yabai -m window --focus east # ;

hyper - n : yabai -m space --create; yabai -m space --focus last
hyper - x : yabai -m space --destroy
# create a new space and throw focused window into it
hyper - m : yabai -m space --create; yabai -m window --space last; yabai -m space --focus last

hyper - i : yabai -m window --space prev; yabai -m space --focus prev
hyper - o : yabai -m window --space next; yabai -m space --focus next

hyper - r : yabai -m space --rotate 90

hyper - left : yabai -m space --swap prev
hyper - right : yabai -m space --swap next

ralt - 1 : yabai -m space --focus 1
ralt - 2 : yabai -m space --focus 2
ralt - 3 : yabai -m space --focus 3
ralt - 4 : yabai -m space --focus 4
ralt - 5 : yabai -m space --focus 5
ralt - 6 : yabai -m space --focus 6
ralt - 7 : yabai -m space --focus 7
ralt - 8 : yabai -m space --focus 8
ralt - 9 : yabai -m space --focus 9
